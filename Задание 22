def to_int(text_number):
# Словарь, соответствующий числам на русском языке
russian_numbers = {
 "ноль": 0, "один": 1, "два": 2, "три": 3, "четыре": 4,
"пять": 5, "шесть": 6, "семь": 7, "восемь": 8, "девять": 9,
"десять": 10, "одиннадцать": 11, "двенадцать": 12, "тринадцать": 13, "четырнадцать": 14,
"пятнадцать": 15, "шестнадцать": 16, "семнадцать": 17, "восемнадцать": 18, "девятнадцать": 19,
"двадцать": 20, "тридцать": 30, "сорок": 40, "пятьдесят": 50, "шестьдесят": 60,
"семьдесят": 70, "восемьдесят": 80, "девяносто": 90, "сто": 100, "двести": 200, "триста": 300,
"четыреста": 400, "пятьсот": 500, "шестьсот": 600, "семьсот": 700, "восемьсот": 800, "девятьсот": 900
 }
 # Разделите текст на слова
 words = text_number.split()
 # Инициализируйте переменные для хранения промежуточных значений
result = 0
temp = 0
# Переберите слова в тексте
for word in words:
if word in russian_numbers:
# Если слово в словаре russian_numbers, добавьте его значение к промежуточному результату
temp += russian_numbers[word]
else:
# Если слово не в словаре, выведите сообщение об ошибке и верните -1
print(f"{word} - некорректное слово в числе.")
return -1
# Если промежуточное значение больше или равно 1000, верните -1
if temp >= 1000:
print("Число слишком большое.")
return -1
# Добавьте промежуточное значение к общему результату
result += temp
return result
# Примеры использования функции
number1 = to_int('тридцать три')
print(number1)
number2 = to_int("сто сорок тридцать два сто")
print(number2)
